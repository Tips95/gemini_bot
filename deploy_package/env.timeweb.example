# Конфигурация для Timeweb Cloud

# Telegram Bot
BOT_TOKEN=your_telegram_bot_token

# Replicate API (для генерации изображений)
REPLICATE_API_KEY=your_replicate_api_key

# YooKassa (платежи)
YOOKASSA_SHOP_ID=your_yookassa_shop_id
